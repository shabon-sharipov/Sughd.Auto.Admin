<MudDialog>
    <DialogContent>
        <MudText>Car images</MudText>
            @foreach (var image in Images)
            {
                <MudImage Src="@image" Width="332" Height="250" Alt="Swedish Farm House" Elevation="25" Class="rounded-lg ma-4"/>
            }
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    
    [Parameter] public List<string> Images { get; set; }

    void Cancel() => MudDialog.Cancel();
}